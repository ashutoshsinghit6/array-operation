# array-operation
#max element of array
arr=[4,52,46,2,3,6]
max=arr[0]
n=len(arr)
min=arr[0]
for i in range(1,n):
    if arr[i]>max:
        max=arr[i]
print("max value",max)

#finding min elements
arr=[4,52,46,2,3,6]
min=arr[0]
n=len(arr)
for i in range(1,n):
    if arr[i]<min:
        min=arr[i]
print("min element",min)
